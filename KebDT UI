		UICorner_3.Parent = Stroke
		local function NSQJYC_script() -- Go.ColorRider 
			local script = Instance.new('LocalScript', Go)
			local coloradd = 0.1
			local tweentime = 0.2
			local TS = game:GetService("TweenService")
			local redP = script.Parent.ImageColor3.R+coloradd
			local greenP = script.Parent.ImageColor3.G+coloradd
			local blueP = script.Parent.ImageColor3.B+coloradd
			local redC = script.Parent.ImageColor3.R+coloradd*2
			local greenC = script.Parent.ImageColor3.G+coloradd*2
			local blueC = script.Parent.ImageColor3.B+coloradd*2
			local red = script.Parent.ImageColor3.R
			local green = script.Parent.ImageColor3.G
			local blue = script.Parent.ImageColor3.B
			script.Parent.MouseEnter:Connect(function()
				TS:Create(script.Parent,TweenInfo.new(tweentime),{ImageColor3 = Color3.new(redP,greenP,blueP)}):Play()
			end)
			script.Parent.MouseLeave:Connect(function()
				TS:Create(script.Parent,TweenInfo.new(tweentime),{ImageColor3 = Color3.new(red,green,blue)}):Play()
			end)
			script.Parent.InputBegan:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseButton1 then
					TS:Create(script.Parent,TweenInfo.new(tweentime),{ImageColor3 = Color3.new(redC,greenC,blueC)}):Play()
				end
			end)
			script.Parent.InputEnded:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseButton1 then
					TS:Create(script.Parent,TweenInfo.new(tweentime),{ImageColor3 = Color3.new(red,green,blue)}):Play()
				end
			end)
		end
		coroutine.wrap(NSQJYC_script)()
		return TextBoxN
	end
