	local function PPQOLGT_script()
		local script = Instance.new('LocalScript', Collapse)
		script.Parent.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				script.Parent.Parent.Parent:TweenPosition(UDim2.new(0.3, 0,1, 0),Enum.EasingDirection.Out,Enum.EasingStyle.Bounce,0.75)
				script.Parent.Parent.Parent.Parent.Expand:TweenPosition(UDim2.new(0.483,0,0.115,0),Enum.EasingDirection.Out,Enum.EasingStyle.Bounce,0.75)
			end
		end)
	end
	coroutine.wrap(PPQOLGT_script)()
	local function EBKRVJU_script()
		local script = Instance.new('LocalScript', Tab1)
		script.Parent.MouseButton1Click:Connect(function()
			for i,v in pairs(script.Parent.Parent.Parent.Buttons:GetChildren()) do
				v.Parent = script.Parent.Parent.Parent.Tabs
				for a,b in pairs(v:GetChildren()) do
					if not b:IsA("IntValue") then
						b.Visible = false
					end
					if b:FindFirstChild("Buttons") then
						b.Buttons.Visible = false
					end
				end
			end
			script.Parent.Parent.Parent.Tabs[script.Parent.Name].Parent = script.Parent.Parent.Parent.Buttons
			for i,v in pairs(script.Parent.Parent.Parent.Buttons[script.Parent.Name]:GetChildren()) do
				if not v:IsA("IntValue") then
					v.Visible = true
				end
			end
		end)
	end
	coroutine.wrap(EBKRVJU_script)()
	return Lib, MainFrame
end
local Libb, MainFM = StartLib()
function Library:DraggingEnabled(parent)
	local frame = MainFM
	--stolen from kavo that stolen from wally loool
	parent = frame
	local dragging = false
	local dragInput, mousePos, framePos
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			mousePos = input.Position
			framePos = parent.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	frame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement then
			dragInput = input
		end
	end)
	input.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			local delta = input.Position - mousePos
			parent.Position  = UDim2.new(framePos.X.Scale, framePos.X.Offset + delta.X, framePos.Y.Scale, framePos.Y.Offset + delta.Y)
		end
	end)
end
function Library:NewTab(name)
	local function CTabb()
		local Tab1 = Instance.new("Folder")
		Tab1.Name = "Tab1"
		local Int = Instance.new("IntValue",Tab1)
		Int.Name = "Buttons"
		Int.Value = -1
		return Tab1
	end
	local function gettTab()
		local Tab1 = Instance.new("TextButton")
		local UICorner = Instance.new("UICorner")
		Tab1.Name = "Tab1"
		Tab1.Parent = MainFM.TabsS
		Tab1.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Tab1.BackgroundTransparency = 0.900
		Tab1.Position = UDim2.new(0.0181967318, 0, 0.137931034, 0)
		Tab1.Size = UDim2.new(0.0177100133, 50, 0.712201536, 0)
		Tab1.ZIndex = 999999999
		Tab1.Font = "Jura"
		Tab1.Text = "Clear"
		Tab1.TextColor3 = Color3.fromRGB(0, 0, 0)
		Tab1.TextScaled = true
		Tab1.TextSize = 14.000
		Tab1.TextWrapped = true
		UICorner.Parent = Tab1
		local function EPTOYSS_script() -- Tab1.LocalScript 
			local script = Instance.new('LocalScript', Tab1)
			script.Parent.MouseButton1Click:Connect(function()
				for i,v in pairs(script.Parent.Parent.Parent.Buttons:GetChildren()) do
					v.Parent = script.Parent.Parent.Parent.Tabs
					for a,b in pairs(v:GetChildren()) do
						if not b:IsA("IntValue") then
							b.Visible = false
						end
						if b:FindFirstChild("Buttons") then
							b.Buttons.Visible = false
						end
					end
				end
				script.Parent.Parent.Parent.Tabs[script.Parent.Name].Parent = script.Parent.Parent.Parent.Buttons
				for i,v in pairs(script.Parent.Parent.Parent.Buttons[script.Parent.Name]:GetChildren()) do
					if not v:IsA("IntValue") then
						v.Visible = true
					end
				end
			end)
		end
		coroutine.wrap(EPTOYSS_script)()
		return Tab1
	end
	local tabamount = 0
	for i,v in pairs(MainFM.TabsS:GetChildren()) do
		tabamount += 1
	end
	local tab = gettTab()
	tab.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	tab.BackgroundTransparency = 0.9
	MainFM.TabsS.Tab1.BackgroundColor3 = Color3.fromRGB(255,0,0)
	MainFM.TabsS.Tab1.BackgroundTransparency = 0.75
	tab.Parent = MainFM.TabsS
	tab.Name = name
	tab.Text = name
	tab.Position = UDim2.new(tabamount*0.175+0.01,0,0.138,0)
	local tabk = CTabb()
	tabk.Parent = MainFM.Tabs
	tabk.Name = name
	local Tab = {}
	local function CTogglee()
		local ToggleN = Instance.new("TextButton")
		local UICorner = Instance.new("UICorner")
		local State = Instance.new("Frame")
		local UIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
		local UICorner_2 = Instance.new("UICorner")
		local Stroke = Instance.new("Frame")
		local UIStroke = Instance.new("UIStroke")
		local UICorner_3 = Instance.new("UICorner")
		local Bool = Instance.new("BoolValue")
		ToggleN.Name = "ToggleN"
		ToggleN.BackgroundColor3 = Color3.fromRGB(28, 57, 63)
		ToggleN.Position = UDim2.new(0.0810003504, 0, 0.00999998208, 0)
		ToggleN.Size = UDim2.new(0.839999974, 0, 0.170000002, 0)
		ToggleN.AutoButtonColor = false
		ToggleN.Font = "Cartoon"
		ToggleN.Text = "Toggle"
		ToggleN.TextColor3 = Color3.fromRGB(0, 0, 0)
		ToggleN.TextSize = 14.000
		UICorner.Parent = ToggleN
		Bool.Name = "Bool"
		Bool.Parent = ToggleN
		State.Name = "State"
		State.Parent = ToggleN
		State.BackgroundColor3 = Color3.fromRGB(255, 0, 4)
		State.BorderSizePixel = 0
		State.Position = UDim2.new(0.0298350994, 0, 0.264235109, 0)
		State.Size = UDim2.new(0.0505185202, 0, 0.588352859, 0)
		UIAspectRatioConstraint.Parent = State
		UICorner_2.CornerRadius = UDim.new(0, 20)
		UICorner_2.Parent = State
		Stroke.Name = "Stroke"
		Stroke.Parent = ToggleN
		Stroke.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Stroke.BackgroundTransparency = 1.000
		Stroke.Position = UDim2.new(0, 0, -0.0243414044, 0)
		Stroke.Size = UDim2.new(1, 0, 1.02434111, 0)
		UIStroke.Parent = Stroke
		UIStroke.Color = Color3.fromRGB(41, 125, 134)
		UIStroke.Thickness = 3.000
		UICorner_3.Parent = Stroke
		local function FOMN_script() -- ToggleN.ColorRider 
			local script = Instance.new('LocalScript', ToggleN)
			local coloradd = 0.1
			local tweentime = 0.1
			local TS = game:GetService("TweenService")
			local red = script.Parent.BackgroundColor.r
			local green = script.Parent.BackgroundColor.g
			local blue = script.Parent.BackgroundColor.b
			local redP = red+coloradd
			local greenP = green+coloradd
			local blueP = blue+coloradd
			local redC = red+coloradd*2
			local greenC = green+coloradd*2
			local blueC = blue+coloradd*2
			script.Parent.MouseEnter:Connect(function()
				TS:Create(script.Parent,TweenInfo.new(tweentime),{BackgroundColor3 = Color3.new(redP,greenP,blueP)}):Play()
			end)
			script.Parent.MouseLeave:Connect(function()
				TS:Create(script.Parent,TweenInfo.new(tweentime),{BackgroundColor3 = Color3.new(red,green,blue)}):Play()
			end)
			script.Parent.InputBegan:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseButton1 then
					TS:Create(script.Parent,TweenInfo.new(tweentime),{BackgroundColor3 = Color3.new(redC,greenC,blueC)}):Play()
				end
			end)
			script.Parent.InputEnded:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseButton1 then
					TS:Create(script.Parent,TweenInfo.new(tweentime),{BackgroundColor3 = Color3.new(red,green,blue)}):Play()
				end
			end)
		end
		coroutine.wrap(FOMN_script)()
		return ToggleN
	end
