	function Tab:Update()
		tabk.Buttons.Value = -1
		local lastvsize = 0
		local lastvpos = 0
		for i,v in pairs(tabk:GetChildren()) do
			if not v:IsA("IntValue") then
				tabk.Buttons.Value += 1
				v.Position = UDim2.new(v.Position.X.Scale,0,lastvpos + lastvsize + 0.05)
				lastvsize = v.Size.Y.Scale
				lastvpos = v.Position.Y.Scale
			end
		end
	end
	function Tab:NewButton(name,callback)
		callback = callback or function() end
		tabk.Buttons.Value += 1
		local button = CButton()
		button.Parent = tabk
		button.Name = name
		button.Text = name
		Tab:Update()
		if not MainFM.Buttons:FindFirstChild(tabk.Name) then
			button.Visible = false
		end
		button.MouseButton1Click:Connect(function()
			callback()
		end)
	end
	function Tab:NewToggle(name,callback)
		callback = callback or function(options) end
		tabk.Buttons.Value += 1
		local button = CTogglee()
		button.Parent = tabk
		button.Name = name
		button.Text = name
		Tab:Update()
		if not MainFM.Buttons:FindFirstChild(tabk.Name) then
			button.Visible = false
		end
		button.MouseButton1Click:Connect(function()
			if button.Bool.Value == true then
				button.Bool.Value = false
				button.State.BackgroundColor3 = Color3.fromRGB(255,0,0)
			else
				button.Bool.Value = true
				button.State.BackgroundColor3 = Color3.fromRGB(0,255,0)
			end
			return callback(button.Bool.Value)
		end)
	end
	function Tab:NewTextBox(name,callback)
		callback = callback or function(options) end
		tabk.Buttons.Value += 1
		local button = CTBA()
		button.Parent = tabk
		button.Name = name
		button.TextLabel.Text = name
		Tab:Update()
		if not MainFM.Buttons:FindFirstChild(tabk.Name) then
			button.Visible = false
		end
		button.Go.MouseButton1Click:Connect(function()
			return callback(button.TextBox.Text)
		end)
	end
	function Tab:NewLabel(name,text,size,r,g,b)
		tabk.Buttons.Value += 1
		local button = CLLB()
		button.Parent = tabk
		button.Name = name
		button.Text = name
		if text ~= nil then
			button.TextLabel.Text = text
		else
			button.Arrow:Destroy()
		end
		if r and g and b then
			button.BackgroundColor3 = Color3.fromRGB(r,g,b)
		end
		local opened = false
		Tab:Update()
		if not MainFM.Buttons:FindFirstChild(tabk.Name) then
			button.Visible = false
		end
		if not size then
			size = 0.5
		end
		if text ~= nil then
			button.MouseButton1Click:Connect(function()
				if opened == false then
					opened = true
					game:GetService("TweenService"):Create(button,TweenInfo.new(0.3),{TextTransparency = 1}):Play()
					game:GetService("TweenService"):Create(button.Arrow,TweenInfo.new(0.3),{Rotation = 0}):Play()
					button:TweenSize(UDim2.new(button.Size.X.Scale,0,size,0),Enum.EasingDirection.Out,Enum.EasingStyle.Bounce,0.75)
					while task.wait() do
						Tab:Update()
						if button.Size == UDim2.new(button.Size.X.Scale,0,size,0) then
							game:GetService("TweenService"):Create(button.TextLabel,TweenInfo.new(0.3),{TextTransparency = 0}):Play()
							break
						end
					end
				else
					opened = false
					button:TweenSize(UDim2.new(button.Size.X.Scale,0,0.17,0),Enum.EasingDirection.Out,Enum.EasingStyle.Bounce,0.75)
					game:GetService("TweenService"):Create(button.TextLabel,TweenInfo.new(0.3),{TextTransparency = 1}):Play()
					game:GetService("TweenService"):Create(button.Arrow,TweenInfo.new(0.3),{Rotation = 180}):Play()
					while task.wait() do
						Tab:Update()
						if button.Size == UDim2.new(button.Size.X.Scale,0,0.17,0) then
							game:GetService("TweenService"):Create(button,TweenInfo.new(0.3),{TextTransparency = 0}):Play()
							break
						end
					end
				end
			end)
		end
	end
	function Tab:NewSlider(name,min,max,callback)
		callback = callback or function(options) end
		tabk.Buttons.Value += 1
		local button = CSLID()
		button.Parent = tabk
		button.Name = name
		button.TextLabel.Text = name
		button.Slider.TextLabel.Text = min.."/"..max
		if not MainFM.Buttons:FindFirstChild(tabk.Name) then
			button.Visible = false
		end
		local function getvalue(value)
			local mouse = game.Players.LocalPlayer:GetMouse()
			local slider = button.Slider
			local percentage = math.clamp((mouse.X-slider.Black.AbsolutePosition.X)/slider.Black.AbsoluteSize.X,0,1)
			local value = math.floor(((max-min)*(percentage))+min)
			button.Slider.TextLabel.Text = (value.."/"..max)
			button.Slider.Black.White:TweenSize(UDim2.new(percentage,0,1,0),Enum.EasingDirection.Out,Enum.EasingStyle.Linear,0.1)
			return callback(value)
		end
		Tab:Update()
		local dragging = false
		button.Slider.Back.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = true
				while task.wait() do
					if dragging then
						local value = getvalue()
					else
						break
					end
				end
			end
		end)
		button.Slider.Back.InputEnded:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = false
			end
		end)
	end
	function Tab:NewDropdown(name)
		local opened = false
		tabk.Buttons.Value += 1
		local button = CDROP()
		button.Parent = tabk
		button.Name = name
		button.TextLabel.Text = name
		if button.Buttons.Visible == false then
			button.Visible = false
		end
		if not MainFM.Buttons:FindFirstChild(tabk.Name) then
			button.Visible = false
		end
		Tab:Update()
		button.ImageLabel.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				if opened then
					button.Buttons.Visible = false
					game:GetService("TweenService"):Create(button.ButtonsTabDecor,TweenInfo.new(0.5),{BackgroundTransparency = 1}):Play()
					button:TweenSize(UDim2.new(0.84,0,0.17,0),Enum.EasingDirection.Out,Enum.EasingStyle.Bounce,0.5)
					for i= 1,50 do
						task.wait(0.01)
						Tab:Update()
					end
					game:GetService("TweenService"):Create(button.ImageLabel,TweenInfo.new(0.3,Enum.EasingStyle.Exponential,Enum.EasingDirection.Out),{Rotation = 0}):Play()
					opened = false
				else
					button:TweenSize(UDim2.new(0.84,0,1,0),Enum.EasingDirection.Out,Enum.EasingStyle.Bounce,0.5)
					game:GetService("TweenService"):Create(button.ImageLabel,TweenInfo.new(0.3,Enum.EasingStyle.Exponential,Enum.EasingDirection.Out),{Rotation = 180}):Play()
					game:GetService("TweenService"):Create(button.ButtonsTabDecor,TweenInfo.new(0.5),{BackgroundTransparency = 0}):Play()
					for i= 1,50 do
						task.wait(0.01)
						Tab:Update()
					end
					button.Buttons.Visible = true
					opened = true
				end
			end
		end)
		local Drop = {
			buttons = -1,
		}
		local dropdown = button
		function Drop:Update()
			for i,v in pairs(dropdown.Buttons:GetChildren()) do
				Drop.buttons = -1
				local lastvsize = 0
				local lastvpos = 0
				for i,v in pairs(dropdown.Buttons:GetChildren()) do
					if not v:IsA("IntValue") then
						Drop.buttons += 1
						v.Position = UDim2.new(v.Position.X.Scale,0,lastvpos + lastvsize + 0.15)
						lastvsize = v.Size.Y.Scale
						lastvpos = v.Position.Y.Scale
					end
				end
			end
		end
		function Drop:InsertButton(name,callback)
			local button = dropdown.Clones.ButtonN:Clone()
			button.Parent = dropdown.Buttons
			button.Name = name
			button.Text = name
			Drop:Update()
			button.MouseButton1Click:Connect(function()
				callback()
			end)
		end
		return Drop
	end
	return Tab
end
function Library:ChangeSize(x,y)
	MainFM.Size = UDim2.new(0,x,0,y)
end
function Library:ChangeName(Name)
	MainFM.NameTab.TextLabel.Text = Name
end
Library:ChangeSize(427,286)--not recommended. Makes UI worse(Decoration).Basic = 427,286
Library:DraggingEnabled(Libb)
return Library
