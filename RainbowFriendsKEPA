local KS = loadstring(game:HttpGet("https://raw.githubusercontent.com/Keparetiolpxe/rbxscript/main/LandedKeySystem"))()
local function s()
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("RainbowFriends v1.2 KEPA","Ocean")
    local Tab1 = Window:NewTab("Chapter 2")
    local S1 = Tab1:NewSection("Complete Section")
    local lastLight = {}
    local Lightning = game:GetService("Lighting")
    local function updateLight()
        lastLight = {
            ["Brightness"] = Lightning.Brightness,
            ["ClockTime"] = Lightning.ClockTime,
            ["FogEnd"] = Lightning.FogEnd,
            ["GlobalShadows"] = Lightning.GlobalShadows,
            ["OutdoorAmbient"] = Lightning.OutdoorAmbient
        }
    end
    local function espStart()
        for i,v in pairs(workspace:GetChildren()) do
            if v.Name == "LightBulb" or v.Name == "GasCanister" or v.Name == "CakeMix" and v:FindFirstChild("TouchTrigger") and not v:FindFirstChild("hlpt") then
                local hl = Instance.new("Highlight",v)
                hl.Name = "hlpt"
                hl.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
                hl.Enabled = true
            end
        end
        for i,v in pairs(workspace.ignore:GetChildren()) do
            if v.Name == "Looky" and v:FindFirstChild("collision") and game.Players.LocalPlayer.Character ~= nil then
                local hl = Instance.new("Highlight",v)
                hl.Name = "hlpt"
                hl.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
                hl.Enabled = true
            end
        end
    end
    local function espEnd()
        for i,v in pairs(workspace:GetChildren()) do
            if v.Name == "LightBulb" or v.Name == "GasCanister" or v.Name == "CakeMix" and v:FindFirstChild("hlpt") then
                v.hlpt:Remove()
            end
        end
        for i,v in pairs(workspace.ignore:GetChildren()) do
            if v.Name == "Looky" and v:FindFirstChild("collision") then
                v.hlpt:Remove()
            end
        end
    end
    local function monespS()
        for i,v in pairs(workspace.Monsters:GetChildren()) do
            if not v:FindFirstChild("hlpt") then
                local hl = Instance.new("Highlight",v)
                hl.Name = "hlpt"
                hl.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
                hl.Enabled = true
                hl.Adornee = v
                hl.OutlineColor = Color3.fromRGB(255,0,0)
                hl.FillColor = Color3.fromRGB(255,255,255)
                hl.FillTransparency = 0.1
            end
        end
    end
    local function monespE()
        for i,v in pairs(workspace.Monsters:GetChildren()) do
            if v:FindFirstChild("hlpt") then
                v.hlpt:Destroy()
            end
        end
    end
    local nz = (0.00000001)
    updateLight()
    S1:NewToggle("FullBright","Light everywhere",function(b)
        if b == true then
            updateLight()
            Lightning.Brightness = 2
            Lightning.ClockTime = 14
            Lightning.FogEnd = 100000
            Lightning.GlobalShadows = false
            Lightning.OutdoorAmbient = Color3.fromRGB(128, 128, 128)
        else
            Lightning.Brightness = lastLight["Brightness"]
            Lightning.ClockTime = lastLight["ClockTime"]
            Lightning.FogEnd = lastLight["FogEnd"]
            Lightning.GlobalShadows = lastLight["GlobalShadows"]
            Lightning.OutdoorAmbient = lastLight["OutdoorAmbient"]
        end
    end)
    local lastpos = CFrame.new(10,136,0,1,0,0,0,1,0,0,0,1)
    local autoreturn = false
    local autoskip = false
    local autowin = false
    local debounce = true
    local espitems = false
    local espmonsters = false
    local infjump = false
    local jumpenabled = false
    local jp = 50
    local wsr = 16
    local wsrg = false
    local utoc = false
    local function firetouch(part)
        if game.Players.LocalPlayer.Character then return end
        part.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        if utoc then
            local ff = game.Players.LocalPlayer.Character.ChildAdded:Connect(function(t)
                if t:IsA("Tool") then
                    task.wait()
                    game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
                    ff:Disconnect()
                end
            end)
        end
    end
    local function returnitems(vv)
        pcall(function()
            if workspace.GroupBuildStructures:FindFirstChildWhichIsA("Model") then
                path = workspace.GroupBuildStructures:FindFirstChildWhichIsA("Model").Trigger
                lap = path.CFrame
                firetouch(path)
                task.wait()
                path.CFrame = lap
            else
                path = workspace.Map_C2.LookyPen.LookyPenTrigger
                lap = path.CFrame
                firetouch(path)
                task.wait(.1)
                path.CFrame = lap
            end
        end)
    end
    local kvu2 = string.reverse("90II-")
    local function complete()
        pcall(function()
            if debounce then
                debounce = false
                for i,v in pairs(workspace:GetChildren()) do
                    if v.Name == "LightBulb" or v.Name == "GasCanister" or v.Name == "CakeMix" and v:FindFirstChild("TouchTrigger") and game.Players.LocalPlayer.Character ~= nil then
                        firetouch(v.TouchTrigger)
                        if autoreturn then
                            task.wait()
                            returnitems()
                        end
                    end
                end
                for i,v in pairs(workspace.ignore:GetChildren()) do
                    if v.Name == "Looky" and v:FindFirstChild("collision") and game.Players.LocalPlayer.Character ~= nil then
                        firetouch(v.collision)
                        returnitems()
                        task.wait()
                        if autoreturn then
                            task.wait()
                            returnitems()
                        end
                    end
                end
                if autoreturn then
                    task.wait()
                    returnitems()
                end
                debounce = true
            end
        end)
    end
    game:GetService("RunService").Heartbeat:Connect(function()
        pcall(function()
            if autoskip then
                game:GetService("ReplicatedStorage").modules.GameHandler_cl.VoteSkip_cl.Network.VotedYes:FireServer()
            end
            if autowin then
                pcall(function()
                    complete()
                end)
            end
            if espitems then
                espStart()
            end
            if espmonsters then 
                monespS()
            end
        end)
        if wsrg and game.Players.LocalPlayer.Character then
            game.Players.LocalPlayer.Character.WalkSpeed = wsr
        end
    end)
    game:GetService("UserInputService").InputBegan:Connect(function(i)
        if i.KeyCode == Enum.KeyCode.Space then
            if jumpenabled then
                game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Jumping,true)
                game.Players.LocalPlayer.Character.Humanoid.JumpPower = jp
            end
            if infjump and game.Players.LocalPlayer.Character then
                game.Players.LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
        end
    end)
    local thing = "Head"
    S1:NewButton("Collect Items(1-4 task)","",function()
        complete()
    end)
    S1:NewToggle("Unequip Tools on Collect","Not working on Lookies ofc",function(b)
        utoc = b
    end)
    S1:NewToggle("Return items when collected","lookies will be auto",function(bool)
        autoreturn = bool
    end)
    local kvv5 = string.reverse("09IO")
    S1:NewButton("Return Items","",function()
        if game.Players.LocalPlayer.Character then
            returnitems(1)
        end
    end)
    S1:NewToggle("AutoSkip","",function(b)
        autoskip = b
    end)
    S1:NewToggle("AutoComplete","",function(b)
        autowin = b
    end)
    local lu12 = string.reverse("-WOBNAIR-")
    S1:NewToggle("Esp Items","",function(b)
        espitems = b
        if b == false then
            espEnd()
        end
    end)
    local kvv3 = "A48J-"
    S1:NewToggle("Esp Monsters","",function(b)
        espmonsters = b
        if b == false then
            monespE()
        end
    end)
    S1:NewToggle("Enable Jump","",function(b)
        jumpenabled = b
        if not b then
            game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Jumping,false)
            game.Players.LocalPlayer.Character.Humanoid.JumpPower = jp
        end
    end)
    local kadd = "friends"
    S1:NewToggle("Infinite Jump","jump jump",function(b)
        infjump = b
    end)
    S1:NewSlider("Speed hack","",100,1,function(v)
        wsr = v
    end)
    S1:NewToggle("Set Speed","",function(b)
        wsrg = b
    end)
    S1:NewSlider("JumpPower","",100,1,function(v)
        jp = v
    end)
    local kvv1 = "Key-"
    local keyy = kvv1..kvv3..kvv5..kvu2..lu12..kadd
    S1:NewButton("Win last run","Cyan",function()
        if game.Players.LocalPlayer.Character then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1260,-171,530,1,0,0,0,1,0,0,0,1)
        end
    end)
end

KS:Key("Key-A48J-OI90-II09-RAINBOW-friends",s,"https://direct-link.net/669390/rainbow-friends-key-v1","KEY IS IN THAT STRANGE BOX. DON'T DELETE ANY SYMBOLS OF LINK. CTRL + A, CTRL + C")
