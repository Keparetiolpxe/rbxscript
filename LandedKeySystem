	Description.Name = "Description"
	Description.Parent = KeySystem
	Description.BackgroundColor3 = Color3.fromRGB(101, 108, 97)
	Description.BackgroundTransparency = 0.600
	Description.Position = UDim2.new(0.640326977, 0, 0.231578946, 0)
	Description.Size = UDim2.new(0.29155314, 0, 0.615789533, 0)
	Description.Font = Enum.Font.SourceSans
	Description.Text = desc
	Description.TextColor3 = Color3.fromRGB(0, 0, 0)
	Description.TextSize = 14.000
	Description.TextWrapped = true
	UICorner_5.CornerRadius = UDim.new(0, 4)
	UICorner_5.Parent = Description
	local CKB = Instance.new("TextButton")
	local UICornerwww = Instance.new("UICorner")

	--Properties:

	CKB.Name = "CKB"
	CKB.Parent = KeySystem
	CKB.BackgroundColor3 = Color3.fromRGB(101, 108, 97)
	CKB.Position = UDim2.new(0.068119891, 0, 0.610526323, 0)
	CKB.Size = UDim2.new(0.416893721, 0, 0.142105266, 0)
	CKB.Font = Enum.Font.SourceSans
	CKB.Text = "Attempt to copy key"
	CKB.TextColor3 = Color3.fromRGB(0, 0, 0)
	CKB.TextSize = 23.000
	CKB.TextWrapped = true

	UICornerwww.CornerRadius = UDim.new(0, 4)
	UICornerwww.Parent = CKB
	CKB.MouseButton1Click:Connect(function()
		local clip = setclipboard or toclipboard
		if clip then
			setclipboard(LinkBox.Text)
			toclipboard(LinkBox.Text)
		else
			local lt = Description.Text
			Description.Text = "Unable to copy to clipboard. Error: No function found"
			task.wait(5)
			Description.Text = lt
		end
	end)
	local function KFKS_script()
		local script = Instance.new('LocalScript', Close)
		script.Parent.MouseButton1Click:Connect(function()
			script.Parent.Parent.Parent:Remove()
		end)
	end
	coroutine.wrap(KFKS_script)()
	local function CFMQVXJ_script() -- KeySystem.Drag 
		local script = Instance.new('LocalScript', KeySystem)
		local input = game:GetService("UserInputService")
		local TS = game:GetService("TweenService")
		local function EnabledDrag(frame)
			frame = frame or script.Parent
			local dragging = false
			local dragInput, mousePos, framePos
			frame.InputBegan:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseButton1 then
					dragging = true
					mousePos = input.Position
					framePos = frame.Position
					input.Changed:Connect(function()
						if input.UserInputState == Enum.UserInputState.End then
							dragging = false
						end
					end)
				end
			end)
			frame.InputChanged:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseMovement then
					dragInput = input
				end
			end)
			input.InputChanged:Connect(function(input)
				if input == dragInput and dragging then
					local delta = input.Position - mousePos
					local destination = UDim2.new(framePos.X.Scale, framePos.X.Offset + delta.X, framePos.Y.Scale, framePos.Y.Offset + delta.Y)
					TS:Create(frame,TweenInfo.new(0.1),{Position = destination}):Play()
				end
			end)
		end
		EnabledDrag(script.Parent)
	end
	TextButton.MouseButton1Click:Connect(function()
		if KeyInput.Text == key then
			script()
		end
	end)
	coroutine.wrap(CFMQVXJ_script)()
	function Library:Destroy()
		LandedKS:Destroy()
	end
end
local function got()
	print("Thankssssssssssss")
	Library:Destroy()
end
Library:Key("kiy",got,"linkee","hi")
