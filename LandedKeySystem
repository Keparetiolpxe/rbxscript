	Name.TextColor3 = Color3.fromRGB(0, 0, 0)
	Name.TextSize = 28.000
	KeyInput.Name = "KeyInput"
	KeyInput.Parent = KeySystem
	KeyInput.BackgroundColor3 = Color3.fromRGB(101, 108, 97)
	KeyInput.Position = UDim2.new(0.068119891, 0, 0.231578946, 0)
	KeyInput.Size = UDim2.new(0.419618517, 0, 0.142105266, 0)
	KeyInput.ClearTextOnFocus = false
