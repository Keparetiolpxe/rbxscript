local KS = {}
function KS:Key(keyV,call,LinkV,descriptionV,OtherLinkV,textOnCorrect)
	local LandedKS = Instance.new("ScreenGui")
	local MainFrame = Instance.new("Frame")
	local DescriptionFrame = Instance.new("Frame")
	local Description = Instance.new("TextLabel")
	local UICorner = Instance.new("UICorner")
	local KSLabel = Instance.new("TextLabel")
	local DescLabel = Instance.new("TextLabel")
	local CopyButton = Instance.new("TextButton")
	local UICorner_2 = Instance.new("UICorner")
	local CheckButton = Instance.new("TextButton")
	local UICorner_3 = Instance.new("UICorner")
	local KeyBox = Instance.new("TextBox")
	local UICorner_4 = Instance.new("UICorner")
	local UICorner_5 = Instance.new("UICorner")
	local LinkBox = Instance.new("TextBox")
	local UICorner_6 = Instance.new("UICorner")
	local OtherLink = Instance.new("TextBox")
	local UICorner_7 = Instance.new("UICorner")
	local OtherCopy = Instance.new("TextButton")
	local UICorner_8 = Instance.new("UICorner")
	LandedKS.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
	LandedKS.Parent = game:GetService("CoreGui")
	LandedKS.ResetOnSpawn = false
	MainFrame.Name = "MainFrame"
	MainFrame.Parent = LandedKS
	MainFrame.BackgroundColor3 = Color3.fromRGB(67, 67, 67)
	MainFrame.Position = UDim2.new(0.328520417, 0, 0.293817967, 0)
	MainFrame.Size = UDim2.new(0, 421, 0, 242)
	DescriptionFrame.Name = "DescriptionFrame"
	DescriptionFrame.Parent = MainFrame
	DescriptionFrame.BackgroundColor3 = Color3.fromRGB(78, 80, 78)
	DescriptionFrame.Position = UDim2.new(0.624703109, 0, 0.148760334, 0)
	DescriptionFrame.Size = UDim2.new(0, 145, 0, 181)
	Description.Name = "Description"
	Description.Parent = DescriptionFrame
	Description.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Description.BackgroundTransparency = 1.000
	Description.Position = UDim2.new(0.068965517, 0, 0.0572916642, 0)
	Description.Size = UDim2.new(0.862068951, 0, 0.880208313, 0)
	Description.Font = Enum.Font.SourceSans
	Description.Text = descriptionV or "No Description"
	Description.TextColor3 = Color3.fromRGB(0, 0, 0)
	Description.TextSize = 14.000
	Description.TextWrapped = true
	Description.TextXAlignment = Enum.TextXAlignment.Left
	Description.TextYAlignment = Enum.TextYAlignment.Top
	UICorner.CornerRadius = UDim.new(0, 4)
	UICorner.Parent = DescriptionFrame
	KSLabel.Name = "KSLabel"
	KSLabel.Parent = MainFrame
	KSLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	KSLabel.BackgroundTransparency = 1.000
	KSLabel.Size = UDim2.new(0, 186, 0, 28)
	KSLabel.Font = Enum.Font.SourceSans
	KSLabel.Text = "KeySystem"
	KSLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
	KSLabel.TextScaled = true
	KSLabel.TextSize = 14.000
	KSLabel.TextWrapped = true
	DescLabel.Name = "DescLabel"
	DescLabel.Parent = MainFrame
	DescLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	DescLabel.BackgroundTransparency = 1.000
	DescLabel.Position = UDim2.new(0.667458415, 0, 0.0123966942, 0)
	DescLabel.Size = UDim2.new(0, 109, 0, 22)
	DescLabel.Font = Enum.Font.SourceSans
	DescLabel.Text = "Description"
	DescLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
	DescLabel.TextScaled = true
	DescLabel.TextSize = 14.000
	DescLabel.TextWrapped = true
	CopyButton.Name = "CopyButton"
	CopyButton.Parent = MainFrame
	CopyButton.BackgroundColor3 = Color3.fromRGB(78, 80, 78)
	CopyButton.Position = UDim2.new(0.0500000007, 0, 0.596000016, 0)
	CopyButton.Size = UDim2.new(0, 219, 0, 29)
	CopyButton.Font = Enum.Font.SourceSans
	CopyButton.Text = "Copy Key Link"
	CopyButton.TextColor3 = Color3.fromRGB(0, 0, 0)
	CopyButton.TextSize = 14.000
	UICorner_2.CornerRadius = UDim.new(0, 5)
	UICorner_2.Parent = CopyButton
	CheckButton.Name = "CheckButton"
	CheckButton.Parent = MainFrame
	CheckButton.BackgroundColor3 = Color3.fromRGB(78, 80, 78)
	CheckButton.Position = UDim2.new(0.0498812348, 0, 0.297520667, 0)
	CheckButton.Size = UDim2.new(0, 219, 0, 29)
	CheckButton.Font = Enum.Font.SourceSans
	CheckButton.Text = "Check Key"
	CheckButton.TextColor3 = Color3.fromRGB(0, 0, 0)
	CheckButton.TextSize = 14.000
	UICorner_3.CornerRadius = UDim.new(0, 5)
	UICorner_3.Parent = CheckButton
	KeyBox.Name = "KeyBox"
	KeyBox.Parent = MainFrame
	KeyBox.BackgroundColor3 = Color3.fromRGB(78, 80, 78)
	KeyBox.Position = UDim2.new(0.0498812348, 0, 0.148760334, 0)
	KeyBox.Size = UDim2.new(0, 219, 0, 29)
	KeyBox.ClearTextOnFocus = false
	KeyBox.Font = Enum.Font.SourceSans
	KeyBox.PlaceholderColor3 = Color3.fromRGB(62, 62, 62)
	KeyBox.PlaceholderText = "Input Key Here"
	KeyBox.Text = ""
	KeyBox.TextColor3 = Color3.fromRGB(0, 0, 0)
	KeyBox.TextSize = 14.000
	UICorner_4.CornerRadius = UDim.new(0, 5)
	UICorner_4.Parent = KeyBox
	UICorner_5.CornerRadius = UDim.new(0, 4)
	UICorner_5.Parent = MainFrame
	LinkBox.Name = "LinkBox"
	LinkBox.Parent = MainFrame
	LinkBox.BackgroundColor3 = Color3.fromRGB(78, 80, 78)
	LinkBox.Position = UDim2.new(0.0500000007, 0, 0.446999997, 0)
	LinkBox.Size = UDim2.new(0, 219, 0, 29)
	LinkBox.ClearTextOnFocus = false
	LinkBox.TextEditable = false
	LinkBox.Font = Enum.Font.SourceSans
	LinkBox.PlaceholderColor3 = Color3.fromRGB(62, 62, 62)
	LinkBox.PlaceholderText = "No Link"
	LinkBox.Text = LinkV or "https://raw.githubusercontent.com/Keparetiolpxe/rbxscript/main/LandedKeySystem"
	LinkBox.TextColor3 = Color3.fromRGB(0, 0, 0)
	LinkBox.TextScaled = true
	LinkBox.TextSize = 14.000
	LinkBox.TextWrapped = true
	UICorner_6.CornerRadius = UDim.new(0, 5)
	UICorner_6.Parent = LinkBox
	OtherLink.Name = "OtherLink"
	OtherLink.Parent = MainFrame
	OtherLink.BackgroundColor3 = Color3.fromRGB(78, 80, 78)
	OtherLink.Position = UDim2.new(0.0499999709, 0, 0.745000064, 0)
	OtherLink.Size = UDim2.new(0, 135, 0, 26)
	OtherLink.TextEditable = false
	OtherLink.ClearTextOnFocus = false
	OtherLink.Font = Enum.Font.SourceSans
	OtherLink.PlaceholderColor3 = Color3.fromRGB(62, 62, 62)
	OtherLink.PlaceholderText = "No Link"
	OtherLink.Text = OtherLinkV or "https://discord.gg/Mb7xc9nEsB"
	OtherLink.TextColor3 = Color3.fromRGB(0, 0, 0)
	OtherLink.TextScaled = true
	OtherLink.TextSize = 14.000
	OtherLink.TextWrapped = true
	UICorner_7.CornerRadius = UDim.new(0, 5)
	UICorner_7.Parent = OtherLink
	OtherCopy.Name = "OtherCopy"
	OtherCopy.Parent = MainFrame
	OtherCopy.BackgroundColor3 = Color3.fromRGB(78, 80, 78)
	OtherCopy.Position = UDim2.new(0.396793306, 0, 0.736495852, 0)
	OtherCopy.Size = UDim2.new(0, 72, 0, 29)
	OtherCopy.Font = Enum.Font.SourceSans
	OtherCopy.Text = "Copy Discord(NOT KEY)"
	OtherCopy.TextColor3 = Color3.fromRGB(0, 0, 0)
	OtherCopy.TextSize = 14.000
	OtherCopy.TextWrapped = true
	UICorner_8.CornerRadius = UDim.new(0, 5)
	UICorner_8.Parent = OtherCopy
	CheckButton.MouseButton1Click:Connect(function()
		if KeyBox.Text == keyV then
			Description.Text = textOnCorrect or "Thank you for using our script"
			task.wait(1)
			call()
			LandedKS:Destroy()
		end
	end)
	CopyButton.MouseButton1Click:Connect(function()
		local clip = setclipboard or toclipboard
		if clip then
			setclipboard(LinkV)
			toclipboard(LinkV)
		else
			Description.Text = "Error while copying 'Text', no command"
			task.wait(3)
			Description.Text = descriptionV
		end
	end)
	OtherCopy.MouseButton1Click:Connect(function()
		local clip = setclipboard or toclipboard
		if clip then
			setclipboard(OtherLinkV)
			toclipboard(OtherLinkV)
		else
			Description.Text = "Error while copying 'Text', no command"
			endtask.wait(3)
			Description.Text = descriptionV
		end
	end)
end
return KS
